{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard",
    "airbnb-base"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.5.1"
    }
  },
  "rules": {
    "no-console": 0,
    "no-else-return": 0,
  }
}